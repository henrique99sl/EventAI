# Banco de dados
DATABASE_URL=postgresql://eventos_user:eventos_pass@localhost:5433/eventos_db

# Ambiente: development, production ou testing
FLASK_ENV=development

# Chave secreta Flask
SECRET_KEY=coloca_aqui_uma_secret_key

# Chave secreta para JWT (pode ser diferente da SECRET_KEY)
JWT_SECRET_KEY=coloca_aqui_uma_jwt_key

# Configuração do CORS (se aplicável)
CORS_ORIGINS=http://localhost:3000

# Sentry DSN para monitorização (opcional)
SENTRY_DSN=